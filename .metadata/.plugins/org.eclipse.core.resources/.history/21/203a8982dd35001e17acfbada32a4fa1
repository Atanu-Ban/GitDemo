import java.util.ArrayList;

public class Array_list {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		ArrayList<String> name = new ArrayList<String>();
		
		name.add("Atanu");
		name.add("Rohan");
		name.add("Anup");
		
		System.out.println(name.get(2));
				

	}

}
